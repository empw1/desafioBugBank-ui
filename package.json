    "test": "echo \"Error: no test specified\" && exit 1",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:smoke": "cypress run --env grepTags='@smoke'",
    "cy:run:regressivo": "cypress run --env grepTags='@regressivo'",
    "cy:run:cadastro": "cypress run --env grepTags='@cadastro'",
    "cy:run:login": "cypress run --env grepTags='@login'",
    "cy:run:transferencia": "cypress run --env grepTags='@transferencia'",
    "report:merge": "mochawesome-merge cypress/reports/*.json > mochawesome.json",
    "report:generate": "marge mochawesome.json -f report -o ./html-report",
    "test:all": "npm run cy:run && npm run report:merge && npm run report:generate"
